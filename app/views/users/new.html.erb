<h1>Register</h1>
<%= form_for :user, url: '/users' do |form| %>
  First Name:<%= form.text_field :first_name %>
  Last Name: <%= form.text_field :last_name %>
  Email: <%= form.text_field :email %>
  Password: <%= form.password_field :password %>
  Confirm Password: <%= form.password_field :confirm_password %>
  <%= form.submit "Submit" %>
  <% end %>
